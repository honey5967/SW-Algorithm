#include <stdio.h>
#include <map>
#include <algorithm>
#include <vector>
using namespace std;

map<string, int> visit;
int ans = 0;

int dfs(string str, vector<char> st, int select)
{
    //해당 숫자 방문 체크
	visit[str] = 1;

	//소수 판별 
	int a = atoi(str.c_str());
	if (a != 0 && a !=1)
	{
		int cnt = 0;
		for (int i = 2; i < a; i++)
		{
			if (a%i == 0)
				cnt++;
		}
		if (cnt == 0)
			ans++;
	}
	

	

    //숫자 선택
	for (int i = 0; i < st.size(); i++)
	{
		if (st[i] != '0' == 0 && select == 0) //처음 0만 방지
		{
			continue;
        }	
		string tmp = str;
		tmp += st[i];

		vector<char>temp = st;
		temp.erase(temp.begin() + i);

		if(visit[tmp]!=1)   //해당 string 방문하지 않았으면 
		    dfs(tmp, temp, select + 1);

	}



	return 0;
}
